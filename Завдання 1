a = int(input ("Введіть а: "))

while (a < 1):

    a = int(input ("Число а повинно бути додатнім: "))

b = int(input ("Введіть b: "))

while (b < 1):

    b = int(input ("Число b повинно бути додатнім: "))

if a < b:

    x = ((a**3 - 5) / 5)

elif a == b:

    x = - 25

else:

    x = (a / b - 1)

print("Результат обчислення виразу: " , x)
